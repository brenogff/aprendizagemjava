import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
	
		Scanner sc = new Scanner(System.in);
		int x;
		int y;
		int inicio = 24;
		int resultado;
		
		System.out.println("Digite a hora inicial:");
		x = sc.nextInt();
		System.out.println("Digite a hora final:");
		y = sc.nextInt();
		
		if(x > y) {
			resultado = inicio - x + y;
			System.out.printf ("A duração do jogo foi de %d horas" ,resultado);}
		
		else {
			resultado = y - x;
			System.out.printf ("A duração do jogo foi de %d horas",resultado);}
		
		sc.close();
	}
}
